        <name>dynamicsStateFirstDerivative</name>
        <reimplements refid="classiDynTree_1_1optimalcontrol_1_1DynamicalSystem_1ae107a97a87ddc0f796e169c6de2cc39b">dynamicsStateFirstDerivative</reimplements>
        <param>
